/* components/Tabs/Tabs.wxss */

.tabs_title {
  display: flex;
  padding: 10rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  background: #fff;
}

.tabs_item {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0;
  border-bottom: 5rpx solid transparent;
}

.active {
  color: #fba414;
  border-bottom: 5rpx solid #fba414;
}
.tabs_content{
  margin-top: 100rpx;
}