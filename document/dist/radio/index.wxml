<view class="i-class i-radio" catchtap="radioChange">
    <i-cell i-class="i-radio-cell">
        <label>
            <radio value="{{value}}" checked="{{checked}}" color="{{checked?color:''}}" disabled="{{disabled}}" class="i-radio-radio {{positionCls}}" />
            <!-- <view class="i-radio-title">{{datas.userName}}</view> -->
            <view class="i-radio-title">
                <text class="">{{addressdatas.userName}}</text>
                <text class="">{{addressdatas.telNumber}}</text>
            </view>
            <text class="">
                {{addressdatas.provinceName+addressdatas.cityName+addressdatas.countyName+addressdatas.detailInfo}}
            </text>
        </label>
    </i-cell>
</view>