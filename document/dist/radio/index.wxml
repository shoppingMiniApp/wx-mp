<view class="i-class i-radio" catchtap="radioChange">
    <i-cell i-class="i-radio-cell">
        <label>
            <radio value="{{value}}" checked="{{checked}}" color="{{checked?color:''}}" disabled="{{disabled}}"
                class="i-radio-radio {{positionCls}}" />
            <!-- <view class="i-radio-title">{{datas.userName}}</view> -->
            <view class="i-radio-title">
                <view class="text-address">
                    <view class="names">{{addressdatas.name}}</view>
                    <view class="phoen">{{addressdatas.phone}}</view>
                </view>
                <view class="detailed">
                    {{addressdatas.procince+addressdatas.city+addressdatas.area+addressdatas.detailed}}
                </view>
            </view>

        </label>
    </i-cell>
</view>