.mine {
  width: 100%;
  margin: 0;
}

.loginPanel {
  width: 100%;
  margin-bottom: 20rpx;
  background: #fef5e780;
}

.login {
  display: flex;
  align-items: center;
  color: rgb(51, 48, 48);
  background: #fef5e780;
  width: 100%;
  height: 100%;
  text-align: left;
  padding: 58rpx 50rpx;
  font-size: 18px;
  font-weight: normal;
  box-sizing: border-box;
}

.login_text {
  margin: 0;
  padding: 0;
}

.mine .loginBtn {
  width: 100%;
  height: 100%;
  text-align: left;
  padding: 50rpx;
  background: #fef5e780 !important;
  font-size: 18px;
  font-weight: normal;
}

.avatar {
  margin-right: 15rpx;
}

.shopHistory {
  width: 100%;
  background: #fef5e780 !important;
}

.historyHeader {
  display: flex;
  border-bottom: 2rpx solid #eee8;
  justify-content: space-between;
  padding: 20rpx 0;
}

.headeright {
  display: flex;
  align-items: center;
}

.headerLeft {
  display: flex;
  align-items: center;
}

.icon {
  padding: 0;
  margin: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.shopStatus {
  width: 100%;
  display: flex;
  padding: 50rpx 20rpx;
  justify-content: space-between;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee8;
}

.shopStatusIcon {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.generalPanel {
  display: flex;
  border-bottom: 2rpx solid #eee8;
  justify-content: space-between;
  padding: 20rpx 0;
}

.generalHead {
  display: flex;
  /* flex-direction: column; */
  align-items: center;
}

.usermotto {
  margin-top: 200px;
}

/* search */
.search-part {
  width: 722rpx;
  padding: 15rpx;
  z-index: 55;
  background-color: #fff;
}

.search-input {
  border-radius: 5px;
  background: rgb(238, 238, 243);
  padding: 12rpx 0 12rpx 49rpx;
  font-size: 28rpx;
  position: relative;
}

.search-icon {
  position: absolute;
  top: 0;
  z-index: 5;
  padding: 31rpx 10rpx;
}

/* 回到顶部 */
.goTopBtn {
  width: 90rpx;
  height: 90rpx;
  background: #fba414;
  position: fixed;
  bottom: 210rpx;
  right: 30rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  text-align: center;
  color: #666;
  z-index: 10;
}

.goTopBtn b {
  font-size: 25rpx;
  color: #fff;
}

/* 分类样式 */
.nav {
  width: 100%;
  height: 60rpx;
  white-space: nowrap;
  /* overflow: hidden; */
  position: relative;
  z-index: 10;
}

.fixed {
  position: fixed;
  top: 0;
  z-index: 10;
  background-color: #fff;
}

.xxxx .fixeds {
  width: 100%;
  position: fixed;
  top: 60rpx;
  z-index: 99;
  background-color: #fff;
}

.nav_view {
  display: inline-block;
  padding: 10rpx 16rpx;
  white-space: normal;
  text-align: center;
  margin: 0 6rpx;
  /* position: relative; */
}

.navborder {
  border-bottom: 4rpx solid #fba414;
  color: #fba414;
}

.navbordernone {
  border: none;
  color: #000;
}

.promise .message {
  height: 50rpx;
  line-height: 50rpx;
}

.xxxx {
  position: relative;
}

.nameChild {
  position: absolute;
  top: 100%;
  left: 0;
  display: flex;
}

.nameChild .nameS {
  padding: 0 20rpx;
  color: #000;
}

.nameChild .navborders {
  border-bottom: 4rpx solid #fba414;
  color: #fba414;
}

/* 轮播 */
.swiper {
  width: 100%;
}

.swiper-item {
  width: 100%;
}

.swiper-item-image {
  width: 100%;
  object-fit: cover;
}

/* recommended */
.recommended {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding-bottom: 16rpx;
  /* border-bottom: 1rpx solid #9998; */
}

.re-v {
  width: 25%;
  height: 218rpx;
  overflow: hidden;
  padding: 16rpx 8rpx 0rpx 8rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 24rpx;
  box-sizing: border-box;
}

.retext {
  padding: 0;
  margin: 0;
  text-align: center;
  margin-top: -20rpx;
}

.re-v-image {
  width: 100%;
  object-fit: cover;
}

/* main */
.main {
  margin-top: -4rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0rpx 12rpx 115rpx 12rpx;
  background-color: #ddd5;
}

.main-view {
  width: 49%;
  border-radius: 10rpx;
  margin-top: 16rpx;
  box-shadow: 0 0 6rpx #ccc7;
  position: relative;
  /* padding: 0 1px; */
  overflow: hidden;
  box-sizing: border-box;
}

.img {
  width: 100%;
  height: 360rpx;
  overflow: hidden;
  margin-bottom: 6px;
}

.img-image {
  display: block;
  width: 100%;
  height: 352rpx;
  object-fit: cover;
}

.text {
  padding: 0 4rpx 10rpx 0;
  margin-top: -20rpx;
  background-color: #fff;
}

.info {
  font-size: 28rpx;
  padding: 10rpx 15rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  color: #555;
  height: 90rpx;
  box-sizing: border-box;
}

.main-vvvv {
  padding: 12rpx 15rpx 0;
  display: flex;
  justify-content: space-between;
}

.main-vvvvv {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.6);
  color: #fba414;
  font-size: 48rpx;
  font-weight: bold;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
}

.price {
  font-weight: bold;
  color: #f00;
}

.price-sup {
  font-size: 25rpx;
}

.hide {
  padding: 0 20rpx;
  font-weight: bold;
  margin-top: -4px;
}

/* 其他分类 商品列表样式*/
.more {
  margin: 60rpx 0 0 0;
  position: relative;
}

.nodata {
  position: absolute;
  top: 300rpx;
  left: 50%;
  transform: translate(-50%, -50%);
}

.classify {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 0 15rpx;
}

.classify-view {
  width: 49%;
  box-shadow: 0 0 6rpx #ccc7;
  border-radius: 10rpx;
  margin-top: 14rpx;
  overflow: hidden;
}

.classify-view-image {
  width: 100%;
  height: 360rpx;
}

.infos {
  padding: 0 15rpx;
}
.register view {
  text-align: center;
  /* padding: 10rpx 0; */
}
.register button {
  padding: 0 !important;
  width: 50% !important;
}

.cart {
  /* padding: 0 25rpx; */
  padding-bottom: 240rpx;
}
.cartTop {
  position: fixed;
  top: 0;
  right: 0rpx;
  left: 0rpx;
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  z-index: 3;
  padding: 0 25rpx;
}
.topTitle {
  padding: 25rpx 0;
  display: flex;
  color: #333;
  font-size: 28rpx;
}
.cartTop button {
  margin: 0 !important;
}
.cartListContent {
  width: 100%;
  padding: 0 25rpx;
  padding-top: 88rpx;
  box-sizing: border-box;
}
.cartList {
  width: 100%;
  display: flex;
  align-items: center;
  background: #eeeeee70;
  border-radius: 10rpx;
  padding: 25rpx 0;
  margin: 25rpx 0;
  box-sizing: border-box;
}
.checkIcon {
  width: 25px;
  height: 25px;
  /* background: #000; */
  border-radius: 50%;
  flex-shrink: 0;
  border: 4rpx solid #fba414;
  /* display: flex;
  align-items: center;
  justify-content: center; */
  margin: 0 25rpx;
  position: relative;
}
.checkIcon .checks {
  position: absolute !important;
  top: -5px !important;
  left: -4.5px !important;
}
.cartItem {
  display: flex;
  width: 100%;
  /* background: coral; */
}
.cartItem image {
  width: 200rpx;
  height: 200rpx;
  border: 2rpx solid #eee;
  background: #fef5e7;
  border-radius: 10rpx;
  /* margin: 0 15rpx; */
  flex-shrink: 0;
  flex-grow: 0;
}
.cartItem_right {
  flex-grow: 1;
  width: 100%;

  display: flex;
  flex-direction: column;
  justify-content: space-between;
  /* background: coral; */
  padding: 0 40rpx;
  box-sizing: border-box;
}
.cartItem_bottom {
  display: flex;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
}
.cartItem_right .cartItem_sku {
  margin: 0;
  width: 200rpx;
}
.cartItem_name {
  width: 340rpx;
  font-size: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.cartItem_number {
  display: flex;
  align-items: center;
  background: #fddaa1;
  border-radius: 6rpx;
  /* height: ; */
}
.itemNum input {
  display: flex;
  align-items: center;
  width: 50rpx;
  height: 40rpx;
  background: #fff;
  font-size: 20rpx;
  padding: 0 5rpx;
  box-sizing: border-box;
  text-align: center;
}
.cartItem_price {
  color: #f00;
  display: flex;
}
.cartItem_price .cartMoneyIcon {
  font-size: 27rpx;
  color: #f00;
  /* display: inline-block; */
}
.cartButton {
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 123rpx;
  z-index: 2;
  padding: 15rpx 26rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}
.cartButton button {
  padding: 0;
  margin: 0;
  width: 250rpx !important;
  /* height: 100rpx; */
  background: #fba414 !important;
  color: #fff !important;
}
.bottomBox {
  background: #fff;
  width: 100%;
  height: 26rpx;
  position: fixed;
  bottom: 0rpx;
  /* z-index: 1; */
}
.cartButtonLeft {
  display: flex;
  align-items: center;
}
.cartButtonRight {
  display: flex;
  align-items: center;
}
.cartButtonRightPrice {
  display: flex;
  width: 200rpx;
  font-size: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.cartButtonRightPrice .cartMoneyIcon {
  color: #f00;
}
.cartButtonRight .cartDelete {
  background: #f06441 !important;
}
.empty {
  width: 100%;
  padding: 400rpx 0;
  text-align: center;
  color: #666;
  /* height: 100%; */
  /* display: flex;
  align-items: center; */
}
.bottomBox {
  display: block;
  background: #fff;
  width: 100%;
  height: 26rpx;
  position: fixed;
  bottom: 0rpx;
  /* z-index: 1; */
}
.navTabBar {
  margin-bottom: 25rpx;
